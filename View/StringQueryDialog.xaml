<Window x:Class="SolutionBuilder.View.StringQueryDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SolutionBuilder.View"
        mc:Ignorable="d"
        Title="StringQueryDialog" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <TextBlock x:Name="TextBlock"  Margin="5"/>
            <TextBox x:Name="QueryStringBox" Margin="5"/>
            <Button Content="Ok" Click="OkButton_Click" Width="50" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
